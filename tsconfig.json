{
  "extends": "astro/tsconfigs/strict",
  "display": "theme/sleek",
  "compilerOptions": {
    "types": ["astro/client"],
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@lib/*": ["./src/lib/*"],
      "@/*": ["./src/*"]
    }
  }
}
